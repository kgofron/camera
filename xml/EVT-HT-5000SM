    <IntSwissKnife Name="SUBS_PARAM">
      <pVariable Name="SUBSAMPLE">pSubSampleReg</pVariable>
      <pVariable Name="SUBS_1X1">SUBS_1X1_VAL</pVariable>
      <pVariable Name="SUBS_2X2">SUBS_2X2_VAL</pVariable>
      <pVariable Name="SUBS_4X4">SUBS_4X4_VAL</pVariable>
      <Formula>(SUBSAMPLE = SUBS_1X1) ? 1 : ((SUBSAMPLE = SUBS_2X2) ? 2 : ((SUBSAMPLE = SUBS_4X4) ? 4 : 8))</Formula>
    </IntSwissKnife>    
    <IntSwissKnife Name="LINE_COUNTS_MIN">
      <pVariable Name="CURPIXFORMAT">pPixelFormatReg</pVariable>
      <pVariable Name="LT_P">LT_PARAM</pVariable>
      <pVariable Name="MLC">MIN_LINE_COUNTS</pVariable>
      <pVariable Name="GVSP_PIX_MONO8">GVSP_PIX_MONO8_VAL</pVariable>
      <pVariable Name="GVSP_PIX_MONO10">GVSP_PIX_MONO10_VAL</pVariable>
      <pVariable Name="GVSP_PIX_MONO12">GVSP_PIX_MONO12_VAL</pVariable>
      <Formula>(CURPIXFORMAT = GVSP_PIX_MONO8) ? MLC : (((CURPIXFORMAT = GVSP_PIX_MONO10) || (CURPIXFORMAT = GVSP_PIX_MONO12)) ? (LT_P*16)/100000 : (LT_P*12)/100000)</Formula>
    </IntSwissKnife>
    <IntSwissKnife Name="LINE_COUNTS_MIN2">
      <pVariable Name="LCM">LINE_COUNTS_MIN</pVariable>
      <pVariable Name="MLC">MIN_LINE_COUNTS</pVariable>
      <Formula>(LCM &gt; MLC) ? LCM : MLC</Formula>
    </IntSwissKnife>
    <IntSwissKnife Name="LINE_COUNTS_MAX">
      <pVariable Name="LT">LINE_COUNTS_MIN2</pVariable>
      <pVariable Name="CURFRAMERATE">pFrameRateReg</pVariable>
      <pVariable Name="M_P">MODEL_PARAM</pVariable>
      <pVariable Name="CURHEIGHT">pHeightReg</pVariable>
      <pVariable Name="SUBS">SUBS_PARAM</pVariable>
      <Formula>(LT &gt; ((150000000/CURFRAMERATE)/(CURHEIGHT*SUBS+M_P))) ? LT : ((150000000000/CURFRAMERATE)/(CURHEIGHT*SUBS+M_P))</Formula>
    </IntSwissKnife>
    <IntSwissKnife Name="FRAME_RATE_MAX_PRE">
      <pVariable Name="LT">LINE_COUNTS_MIN2</pVariable>
      <pVariable Name="M_P">MODEL_PARAM</pVariable>
      <pVariable Name="CURHEIGHT">pHeightReg</pVariable>
      <pVariable Name="MLC">MIN_LINE_COUNTS</pVariable>
      <pVariable Name="SUBS">SUBS_PARAM</pVariable>
      <Formula>(LT &gt; MLC/100) ? (150000000/(LT*(CURHEIGHT*SUBS+M_P))) : (150000000/(MLC*(CURHEIGHT*SUBS+M_P)))</Formula>
    </IntSwissKnife>
    <IntSwissKnife Name="FRAME_RATE_MAX">
      <pVariable Name="CUREXPOSURE">pExposureReg</pVariable>
      <pVariable Name="M_P">MODEL_PARAM</pVariable>
      <pVariable Name="FR_MAX_PRE">FRAME_RATE_MAX_PRE</pVariable>
      <pVariable Name="TRIGGERSRC">TriggerSourceReg</pVariable>
      <pVariable Name="LT">pLineTimeReg</pVariable>
      <Formula>(TRIGGERSRC = 0) ? ((CUREXPOSURE &gt; ((1000000/FR_MAX_PRE)-((M_P*LT)/150))) ? (1000000/(((M_P*LT)/150)+CUREXPOSURE)) : (FR_MAX_PRE)) : (FR_MAX_PRE)</Formula>
    </IntSwissKnife>
    <IntSwissKnife Name="FRAME_RATE_MAX_MHZ">
      <pVariable Name="FRMAX">FRAME_RATE_MAX</pVariable>
      <Formula>FRMAX*1000</Formula>
    </IntSwissKnife>
    <Integer Name="AcquisitionFrameCount">
